{
  "version": 1,
  "projects": {
    "server-ssr": {
      "root": "apps/server/ssr",
      "sourceRoot": "apps/server/ssr/src",
      "projectType": "application",
      "prefix": "server-ssr",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/server/ssr",
            "main": "apps/server/ssr/src/main.ts",
            "tsConfig": "apps/server/ssr/tsconfig.app.json",
            "assets": ["apps/server/ssr/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/ssr/src/environments/environment.ts",
                  "with": "apps/server/ssr/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "server-ssr:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/server/ssr/tsconfig.app.json"],
            "exclude": ["**/node_modules/**", "!apps/server/ssr/**"]
          }
        },
        "deploy": {
          "builder": "@angular-devkit/architect:concat",
          "options": {
            "targets": [
              { "target": "server-ssr:build" }
            ]
          }
        }
      }
    },
    "client-admin": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:class": {
          "skipTests": true
        },
        "@nrwl/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@nrwl/angular:directive": {
          "skipTests": true
        },
        "@nrwl/angular:guard": {
          "skipTests": true
        },
        "@nrwl/angular:module": {
          "skipTests": true
        },
        "@nrwl/angular:pipe": {
          "skipTests": true
        },
        "@nrwl/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/client/admin",
      "sourceRoot": "apps/client/admin/src",
      "prefix": "client-admin",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/client/admin",
            "index": "apps/client/admin/src/index.html",
            "main": "apps/client/admin/src/main.ts",
            "polyfills": "apps/client/admin/src/polyfills.ts",
            "tsConfig": "apps/client/admin/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/client/admin/src/favicon.ico",
              "apps/client/admin/src/assets"
            ],
            "styles": ["apps/client/admin/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/admin/src/environments/environment.ts",
                  "with": "apps/client/admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client-admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-admin:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/client/admin/tsconfig.app.json"],
            "exclude": ["**/node_modules/**", "!apps/client/admin/**"]
          }
        },
        "deploy": {
          "builder": "@angular-devkit/architect:concat",
          "options": {
            "targets": [{ "target": "client-admin:build:production" }]
          }
        }
      }
    },
    "client-portfolio": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:class": {
          "skipTests": true
        },
        "@nrwl/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@nrwl/angular:directive": {
          "skipTests": true
        },
        "@nrwl/angular:guard": {
          "skipTests": true
        },
        "@nrwl/angular:module": {
          "skipTests": true
        },
        "@nrwl/angular:pipe": {
          "skipTests": true
        },
        "@nrwl/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/client/portfolio",
      "sourceRoot": "apps/client/portfolio/src",
      "prefix": "client-portfolio",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/client/portfolio",
            "index": "apps/client/portfolio/src/index.html",
            "main": "apps/client/portfolio/src/main.ts",
            "polyfills": "apps/client/portfolio/src/polyfills.ts",
            "tsConfig": "apps/client/portfolio/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/client/portfolio/src/favicon.ico",
              "apps/client/portfolio/src/assets"
            ],
            "styles": ["apps/client/portfolio/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/portfolio/src/environments/environment.ts",
                  "with": "apps/client/portfolio/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client-portfolio:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-portfolio:build:production:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-portfolio:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/client/portfolio/tsconfig.app.json"],
            "exclude": ["**/node_modules/**", "!apps/client/portfolio/**"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/apps/client/portfolio/server",
            "main": "apps/client/portfolio/src/main.server.ts",
            "tsConfig": "apps/client/portfolio/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/portfolio/src/environments/environment.ts",
                  "with": "apps/client/portfolio/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        },
        "app-shell": {
          "builder": "@angular-devkit/build-angular:app-shell",
          "options": {
            "browserTarget": "client-portfolio:build",
            "serverTarget": "client-portfolio:server",
            "route": "shell"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-portfolio:build:production",
              "serverTarget": "client-portfolio:server:production"
            }
          }
        },
        "deploy": {
          "builder": "@angular-devkit/architect:concat",
          "options": {
            "targets": [{ "target": "client-portfolio:app-shell:production" }]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "client-portfolio"
}
